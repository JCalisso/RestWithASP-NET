{
    "DB": {
        "Connection": "%DB_CONNECTION%"
    },
    "TokenConfigurations": {
        "Audience": "%TOKEN_AUDIENCE%",
        "Issuer": "%TOKEN_ISSUER%",
        "Secret": "%TOKEN_SECRET%", 
        "Minutes": "%TOKEN_MINUTES%",
        "DaysToExpire": "%TOKEN_DAYS_TO_EXPIRE%"
    },

    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*"
}